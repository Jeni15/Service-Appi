    <!-- CERTIFICADODTO -->
    <SQL id="CertificadoDto_Get">
      <!--

       SELECT    c."IDCERTIFICADO",c.NoCCITE,c."FECHAEXPEDICION",c."FECHAVENCIMIENTO",c."IDEMPRESA",c."IDSOLICITUD",c."IDESTADOCERTIFICADO",c."IDPRESOLICITUD",c."IDVIGENCIA",c."PERIODICIDAD",c."CODSEGURIDAD",c."NOCCITESIE1"
               , sc.sucursal  
               , s.sustancia 
               , dc.cantidad
               , u.unidad
               , e.Nombre as NombreEmpresa
               , ec.EstadoCertificado
         FROM    Certificado    c   
                 INNER JOIN Empresa            e  ON  c.IDEmpresa = e.IDEmpresa  
                 INNER JOIN EstadoCertificado  ec ON  c.IDEstadoCertificado = ec.IDEstadoCertificado  
                 INNER JOIN DetalleCertificado dc ON  c.IDCertificado = dc.IDCertificado  
                 INNER JOIN Sustancia          s  ON  dc.IDSustancia = s.IDSustancia 
                 LEFT JOIN Unidad             u  ON  s.IDUnidad = u.IDUnidad 
                 INNER JOIN Sucursal           sc ON  dc.IDSucursal = sc.IDSucursal 
         WHERE NOCCITE = :NoCcite
          ORDER BY e.Nombre, sc.sucursal, s.sustancia
  
      -->
    </SQL>

